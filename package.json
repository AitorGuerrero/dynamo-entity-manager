{
	"name": "dynamo-entity-manager",
	"version": "2.2.0",
	"description": "Entity manager for Amazon DynamoDB NoSQL database",
	"main": "index.js",
	"scripts": {
		"postpublish": "git push",
		"test": "tsc && mocha ./**/*.test.js",
		"build": "tsc",
		"fixStyle": "prettier -c . --write && tslint -p . --fix",
		"preversion": "tsc && nyc mocha"
	},
	"keywords": [
		"aws",
		"amazon",
		"dynamo",
		"dynamodb",
		"entity",
		"manager"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AitorGuerrero/dynamo-entity-manager.git"
	},
	"author": "Aitor Guerrero <aitor.guerrero.dev@gmail.com>",
	"license": "ISC",
	"dependencies": {
		"powered-dynamo": "^2.1.3"
	},
	"devDependencies": {
		"@types/chai": "^4.1.7",
		"@types/mocha": "^8.0.3",
		"@types/node": "^14.6.2",
		"aws-sdk": "^2.407.0",
		"chai": "^4.2.0",
		"husky": "^4.2.1",
		"mocha": "^8.1.3",
		"nyc": "^15.0.0",
		"prettier": "^2.0.5",
		"source-map-support": "^0.5.16",
		"ts-node": "^9.0.0",
		"tslint": "^6.0.0",
		"typescript": "^4.0.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsc && prettier -c . && tslint -p . && nyc mocha"
		}
	}
