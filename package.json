{
  "name": "dynamo-entity-manager",
  "version": "2.1.13",
  "description": "Entity manager for Amazon DynamoDB NoSQL database",
  "main": "index.js",
  "scripts": {
    "postpublish": "git push",
    "test": "mocha ./**/*.test.js",
    "build": "tsc",
    "check:style": "tslint -p .",
    "check:builds": "tsc --noEmit",
    "preversion": "yarn build && yarn coverage",
    "coverage": "nyc mocha ./**/*.test.js",
    "style": "tslint -p ."
  },
  "keywords": [
    "aws",
    "amazon",
    "dynamo",
    "dynamodb",
    "entity",
    "manager"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AitorGuerrero/dynamo-entity-manager.git"
  },
  "author": "Aitor Guerrero <aitor.guerrero.dev@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "powered-dynamo": "^2.1.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.0",
    "aws-sdk": "^2.407.0",
    "chai": "^4.2.0",
    "husky": "^4.2.1",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "tslint": "^6.0.0",
    "typescript": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn && yarn check:style && yarn check:builds && yarn coverage"
    }
  }
}
